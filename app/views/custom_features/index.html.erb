<style>
  #cf_wrap .row {
    margin-bottom: 10px;
  }
</style>
<%-enabled_features = MasterSetup.get.custom_features_list-%>
<div class='container' id='cf_wrap'>
  <div class='row'>
    <div class='col-md-12 text-center'>
      <h1>Custom Features</h1>
    </div>
  </div>
  <%-if enabled_features.blank?-%>
  <div class='row'>
    <div class='col-md-12'>
      There aren't any custom features enabled for this system.
    </div>
  </div>
  <%-end-%>
  <%-if enabled_features.include? 'CSM Sync'%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "CSM Sync", '/custom_features/csm_sync', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end%>
  <%-if enabled_features.include?('Polo Canada') && OpenChain::CustomHandler::PoloCaEntryParser.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Polo Canada", '/custom_features/polo_canada', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end-%>
  <%-if enabled_features.include?('Polo SAP') && OpenChain::CustomHandler::PoloSapBomHandler.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "SAP Bill of Materials", '/custom_features/polo_sap_bom', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end-%>
  <%-if enabled_features.include?('JCrew Parts') && OpenChain::CustomHandler::JCrewPartsExtractParser.new.can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "JCrew Parts Upload", '/custom_features/jcrew_parts', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end-%>
  <%-if enabled_features.include?('Polo CA Invoices') && OpenChain::CustomHandler::Polo::PoloCaInvoiceHandler.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Polo CA Invoice Upload", '/custom_features/polo_ca_invoices', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end-%>
  <%-if enabled_features.include?('UA SAP')-%>
    <%-if OpenChain::CustomHandler::UnderArmour::UaTbdReportParser.new(nil).can_view?(current_user)-%>
      <div class='row'>
        <div class='col-md-4 col-md-offset-4'>
          <%=link_to "TBD Report Upload", '/custom_features/ua_tbd', :class=>'btn btn-lg btn-default btn-block'%>
        </div>
      </div>
    <%-end-%>
    <%-if OpenChain::CustomHandler::UnderArmour::UaWinshuttleProductGenerator.new.can_view?(current_user)-%>
      <div class='row'>
        <div class='col-md-4 col-md-offset-4'>
          <%=link_to "Winshuttle HTS Email", '/custom_features/ua_winshuttle', :class=>'btn btn-lg btn-default btn-block'%>
        </div>
      </div>
    <%-end-%>
    <%-if OpenChain::CustomHandler::UnderArmour::UaWinshuttleScheduleBGenerator.new.can_view?(current_user)-%>
      <div class='row'>
        <div class='col-md-4 col-md-offset-4'>
          <%=link_to "Winshuttle Schedule B Email", '/custom_features/ua_winshuttle_b', :class=>'btn btn-lg btn-default btn-block'%>
        </div>
      </div>
    <%-end-%>
  <%-end-%>
  <%-if enabled_features.include?('Fenix CI Upload') && OpenChain::CustomHandler::FenixCommercialInvoiceSpreadsheetHandler.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Fenix Commerical Invoice Upload", '/custom_features/fenix_ci_load', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end-%>
  <%-if enabled_features.include?('Eddie Fenix CI Upload') && OpenChain::CustomHandler::EddieBauer::EddieBauerFenixInvoiceHandler.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Eddie Fenix Commerical Invoice Upload", '/custom_features/eddie_fenix_ci_load', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end-%>
  <%-if OpenChain::CustomHandler::Lenox::LenoxShipmentStatusParser.can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Lenox OOCL Shipment Report Upload", '/custom_features/lenox_shipment_status', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end%>
  <%-if OpenChain::CustomHandler::EcellerateShipmentActivityParser.can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Ecellerate Shipment Activity Report Upload", '/custom_features/ecellerate_shipment_activity', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end%>
  <%-if OpenChain::CustomHandler::LandsEnd::LeReturnsParser.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Lands' End Returns", '/custom_features/le_returns', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end%>
  <%-if OpenChain::CustomHandler::LandsEnd::LeReturnsCommercialInvoiceGenerator.new(nil).can_view?(current_user)-%>
  <div class='row'>
    <div class='col-md-4 col-md-offset-4'>
      <%=link_to "Lands' End CI Load", '/custom_features/le_ci_load', :class=>'btn btn-lg btn-default btn-block'%>
    </div>
  </div>
  <%-end%>
</div>
