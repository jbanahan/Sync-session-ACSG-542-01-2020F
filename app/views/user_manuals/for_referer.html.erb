<div class='container-fluid'>
  <div class="row">
    <div class="col-xs-10 col-xs-offset-1">
      <%if @manuals.blank?%>
        <div class="alert alert-info">There aren't any user manuals configured for this page.</div>
      <%end%>
      <%@manuals.each do |m|%>
        <%=link_to m.name, download_user_manual_path(m), {:class=>'btn btn-primary btn-block'}%>
      <%end%>
    </div>
  </div>
</div>
