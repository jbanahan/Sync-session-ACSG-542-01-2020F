<h1>Listing attachment_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Module type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @attachment_types.each do |attachment_type| %>
  <tr>
    <td><%= attachment_type.name %></td>
    <td><%= attachment_type.module_type %></td>
    <td><%= link_to 'Show', attachment_type %></td>
    <td><%= link_to 'Edit', edit_attachment_type_path(attachment_type) %></td>
    <td><%= link_to 'Destroy', attachment_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Attachment type', new_attachment_type_path %>
